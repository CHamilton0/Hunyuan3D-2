{
    "project": {
        "root": "Hunyuan3D-2",
        "script": "generate.py",
        "script_args": "--img assets/test_images/horse0.png --output-dir results --config-file gen_config.json"
    },
    "api": {
        "host": "127.0.0.1",
        "port": 8080,
        "limit_model_concurrency": 2
    },
    "data": {
        "img_dir": "assets/test_images",
        "outpur_dir": "results",
        "output_file_type": "glb"
    },
    "inference": {
        "guidance_scale": 7.5,
        "num_chunks": 10000,
        "num_inference_steps": 300,
        "octree_resolution": 384,
        "output_type": "trimesh",
        "seed": 42
    },
    "model": {
        "model_path": "tencent/Hunyuan3D-2mini",
        "subfolder": "hunyuan3d-dit-v2-mini",
        "use_safetensors": false,
        "variant": "fp16",
        "device": "cuda",
        "enable_flashvdm": false
    },
    "post_processing": {
        "max_facenum": 10000
    }
}